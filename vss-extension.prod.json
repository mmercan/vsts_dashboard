{
    "manifestVersion": 1,
    "id": "confidence-extension-prod",
    "version": "1.0.2",
    "name": "Confidence Extension",
    "publisher": "mmercan2",
    "description": "Awesome tools to bring Tests and Work items together",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "scopes": [
        "vso.work",
        "vso.test",
        "vso.release",
        "vso.build",
        "vso.work_write",
        "vso.test_write",
        "vso.profile",
        "vso.code",
        "vso.code_status",
        "vso.dashboards",
        "vso.project",
        "vso.release_execute"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "contributions": [{
        "id": "confidence-dashboard-prod",
        "type": "ms.vss-web.hub",
        "includes": [
            "ms.vss-releaseManagement-web.release-service-data-external"
        ],
        "description": "Awesome dashboard brings linked tests and workitems and give you a confidence score base on linked tests and test results",
        "targets": [
            "ms.vss-web.home-hub-group",
            "ms.vss-test-web.test-hub-group"
        ],
        "properties": {
            "name": "Confidence",
            "title": "Confidence Dashboard",
            "uri": "index.html"
        }
    }],
